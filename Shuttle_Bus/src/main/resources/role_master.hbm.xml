<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.Role_Master" table="role_master">
 	 	
 	   <id name="role_id" type="string">
            <column name="role_id" />
        </id>
        <property name="role_name" type="string">
            <column name="role_name" not-null="true" />
        </property>
        
        <set name="user_master" table="user_master"
				inverse="true" lazy="true" fetch="select">
            <key>
                <column name="role_id" not-null="true" />
            </key>
            <one-to-many class="com.EntityClasses.User_Master" />
        </set>
 	</class>
</hibernate-mapping>